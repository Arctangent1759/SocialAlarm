

<p id="notice"><%= notice %></p>

<p>
  <b>Alarm time:</b>
  <%= @alarm.alarm_time %>
</p>

<p>
  <b>Expected:</b>
  <%= @alarm.expected %>
</p>

<p>
  <b>Sessions:</b>
<% group_users = []%>
  <% for user_id in @alarm.sessions %>
		<%group_users.push(User.find(user_id).username)%>
	<% end %>
	<%= group_users%>
</p>

<%= link_to "Check In", "#{params[:id]}/check_in"%>

<%= link_to 'Edit', edit_alarm_path(@alarm) %> |
<%= link_to 'Back', alarms_path %>
